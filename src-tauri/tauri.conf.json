{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "height": 720,
        "title": "HSLink Nexus",
        "width": 1280
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "../public/HSLink.svg"
    ],
    "resources": {
      "../99-hslink.rules": "99-hslink.rules",
      "../LICENSE": "LICENSE",
      "../README.md": "README.md"
    },
    "targets": "all"
  },
  "identifier": "com.hslinknexus.app",
  "productName": "HSLink Nexus",
  "version": "1.2.2"
}