{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "height": 720,
        "title": "HSLinkUpper",
        "width": 1280
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "pnpm build",
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "/usr/lib/udev/rules.d/": "udev-rules/99-hslink.rules"
    ],
    "targets": "all"
  },
  "identifier": "com.hslinkupper.app",
  "productName": "hslinkupper",
  "version": "1.0.0"
}
